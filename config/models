User
    nickname Text
    password Text Maybe
    apps [TApplication]
    UniqueNickname nickname
    deriving Show Read Eq

Email
    email Text
    user UserId Maybe
    verkey Text Maybe
    UniqueEmail email

TFile
    user User
    rawpath RawPath
    ancestors [Double]
    parent Double Maybe
    filename Text
    content Text Maybe
    contentType Text Maybe
    fileType FileType
    UniqueRawPath rawpath
    permissions [Permission]
    
Permission
    user User
    read Bool
    write Bool
    app TApplication
    deriving

TApplication
    name Text
    description Text
    contactEmail Text    
    repositoryUrl Text Maybe
    creationDate UTCTime
    deriving Show Read Eq
